<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="T2TiERPFenix" namespace="T2TiERPFenix.Models" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Transportadora" table="TRANSPORTADORA" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>

	<property name="DataCadastro" column="DATA_CADASTRO" />
	<property name="Observacao" column="OBSERVACAO" />

    <many-to-one name="Pessoa" class="Pessoa" column="ID_PESSOA" />
    <many-to-one name="ViewPessoaTransportadora" class="ViewPessoaTransportadora" column="ID_PESSOA" not-found="ignore" insert="false" update="false"/>


  </class>
</hibernate-mapping>